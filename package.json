{
  "name": "@johnls/consul-tool",
  "version": "1.1.0",
  "engines": {
    "node": ">=10.0"
  },
  "os": [
    "darwin",
    "linux"
  ],
  "description": "Extended commands for consul CLI",
  "scripts": {
    "build": "rm -rf dist && babel src -d dist -s --ignore **/*.test.js",
    "debug:consul-extra": "babel-node --inspect-brk src/consul-extra.js",
    "test": "jest --coverage",
    "deploy": "npm publish --access public"
  },
  "bin": {
    "consul-extra": "dist/consul-extra.js"
  },
  "files": [
    "dist/**"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jlyonsmith/consul-tool.git"
  },
  "keywords": [
    "consul",
    "cli",
    "tool"
  ],
  "author": "John Lyon-Smith",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jlyonsmith/consul-tool/issues"
  },
  "homepage": "https://github.com/jlyonsmith/consul-tool#readme",
  "dependencies": {
    "autobind-decorator": "^2.4.0",
    "chalk": "^2.4.2",
    "consul": "^0.34.1",
    "flat": "^4.1.0",
    "json5": "^2.1.0",
    "minimist": "^1.2.0",
    "tmp": "0.0.33",
    "tmp-promise": "^1.0.5"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "jest": "^24.1.0"
  }
}
