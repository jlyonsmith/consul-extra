{
  "name": "@johnls/consul-tool",
  "version": "1.1.0",
  "engines": {
    "node": ">=10.0"
  },
  "os": [
    "darwin",
    "linux"
  ],
  "description": "Extended commands for consul CLI",
  "scripts": {
    "build": "rm -rf dist && babel src -d dist -s --ignore **/*.test.js",
    "debug:consul-extra": "babel-node --inspect-brk src/consul-extra.js",
    "test": "jest --coverage",
    "deploy": "npm publish --access public"
  },
  "bin": {
    "consul-extra": "dist/consul-extra.js"
  },
  "files": [
    "dist/**"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jlyonsmith/consul-tool.git"
  },
  "keywords": [
    "consul",
    "cli",
    "tool"
  ],
  "author": "John Lyon-Smith",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jlyonsmith/consul-tool/issues"
  },
  "homepage": "https://github.com/jlyonsmith/consul-tool#readme",
  "dependencies": {
    "autobind-decorator": "^2.4.0",
    "chalk": "^2.4.2",
    "consul": "^0.34.1",
    "flat": "^4.1.0",
    "json5": "^2.1.0",
    "minimist": "^1.2.0",
    "tmp": "0.0.33",
    "tmp-promise": "^1.0.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.3",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.3",
    "@babel/plugin-proposal-decorators": "^7.3.0",
    "@babel/plugin-proposal-optional-chaining": "^7.2.0",
    "@babel/plugin-syntax-object-rest-spread": "^7.2.0",
    "@babel/preset-env": "^7.3.1",
    "jest-cli": "^24.1.0"
  }
}
